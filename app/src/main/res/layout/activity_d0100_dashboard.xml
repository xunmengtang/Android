<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
        <variable
            name="vmDashboard"
            type="com.reaksmeyarun.pda.viewmodel.DashboardViewModel" />
    </data>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".view.activity.D0100DashboardActivity">
        <com.example.myapplication.customview.BaseAppBackground
            android:layout_width="match_parent"
            android:layout_height="match_parent"/>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">
                <include
                    layout="@layout/d0110_dashboard_main_layout"
                    app:vmDashboard = "@{vmDashboard}"/>
            </FrameLayout>
        </LinearLayout>
    </RelativeLayout>
</layout>